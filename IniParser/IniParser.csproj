<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>ErrOwk.IniParser</PackageId>
    <Title>ErrOwk.IniParser</Title>
    <Authors>ErrOwk</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>A simple .net based ini file operation class.</Description>
    <PackageProjectUrl>https://github.com/ErrOwk/IniParser</PackageProjectUrl>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/ErrOwk/IniParser</RepositoryUrl>
    <PackageTags>ini</PackageTags>
    <Version>1.1.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Update="readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
